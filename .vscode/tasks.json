{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Link together",
            "command": "${env:MSYS2_HOME}\\mingw64\\bin\\gcc.exe",
            "args": [
                "-g",
                "-o",
                "out\\main.exe",
                "out\\main.o",
                "out\\binary.o",
                "-lole32",
            ],
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "PATH": "${env:PATH};${env:MSYS2_HOME}\\mingw64\\bin;${env:MSYS2_HOME}\\usr\\bin"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": "C/C++: gcc.exe build main",
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build main",
            "command": "${env:MSYS2_HOME}\\mingw64\\bin\\gcc.exe",
            "args": [
                "-Wall",
                "-ansi",
                "-pedantic",
                "-c",
                "-g",
                "-o",
                "out\\main.o",
                "main.c"
            ],
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "PATH": "${env:PATH};${env:MSYS2_HOME}\\mingw64\\bin;${env:MSYS2_HOME}\\usr\\bin"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build"
            },
            "dependsOn": "Create binary object",
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "Create binary object",
            "command": "${env:MSYS2_HOME}\\mingw64\\bin\\ld.exe",
            "args": [
                "-r",
                "-b",
                "binary",
                "-o",
                "out\\binary.o",
                "binres\\test.bin"
            ],
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "PATH": "${env:PATH};${env:MSYS2_HOME}\\mingw64\\bin;${env:MSYS2_HOME}\\usr\\bin"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build"
            },
            "dependsOn": "Prepare",
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "Prepare",
            "command": "pwsh",
            "args": [
                "-File",
                "Prepare.ps1"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build"
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}